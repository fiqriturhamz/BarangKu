<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="10dp"
    app:cardElevation="10dp"
    android:layout_marginStart="30dp"
    android:layout_marginEnd="30dp"
    android:layout_marginTop="10dp"
    android:layout_height="wrap_content">


<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:padding="10dp"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_nama_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:layout_marginTop="8dp"
        android:text="Nama Barang Keluar : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_id_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_input_nama_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aqua"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_nama_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_nama_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_nama_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_id_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:text="ID : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/tv_input_id_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_id_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_id_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_id_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_tanggal_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:layout_marginTop="8dp"
        android:text="Tanggal Keluar : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nama_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_jenis_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:layout_marginTop="8dp"
        android:text="Jenis Barang : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tanggal_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_input_jenis_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Minuman"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_jenis_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_jenis_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_jenis_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_input_tanggal_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2023"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_tanggal_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_tanggal_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_tanggal_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_input_jumlah_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="99"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_jumlah_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_jumlah_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_jumlah_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_jumlah_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:layout_marginTop="8dp"
        android:text="Jumlah Barang Keluar : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_jenis_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_deskripsi_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/thirty"
        android:layout_marginTop="8dp"
        android:text="Deskripsi : "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_jumlah_barang_keluar_di_barang_keluar">

    </TextView>

    <TextView
        android:id="@+id/tv_input_deskripsi_barang_keluar_di_barang_keluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habis diminum"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_deskripsi_barang_keluar_di_barang_keluar"
        app:layout_constraintStart_toEndOf="@+id/tv_deskripsi_barang_keluar_di_barang_keluar"
        app:layout_constraintTop_toTopOf="@+id/tv_deskripsi_barang_keluar_di_barang_keluar">

    </TextView>
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>